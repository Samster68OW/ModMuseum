<AstroObjectEntry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/shiplog_schema.xsd">
  <ID>TIMBER_HEARTH</ID>



  <Entry> <!-- An Entry For This Planet -->
    <ID>MOD_MUSEUM</ID> <!-- The ID of this entry -->
    <Name>MOD_MUSEUM_ENTRY_NAME</Name> <!-- The name to show for this entry both on the card and in map mode -->
    <Curiosity>MOD_MUSEUM</Curiosity> <!-- The curiosity this entry belongs to (optional) in this case, it belongs to itself -->
    <IsCuriosity/> <!-- Marks this entry as a curiosity, makes it bigger in rumor mode and allows custom colors -->

    <RumorFact>
      <ID>CHIPS_PROJECT_RUMOR</ID>
      <SourceID>TH_VILLAGE</SourceID>
      <RumorName>CHIPS_PROJECT_RUMOR_NAME</RumorName>
      <Text>CHIPS_PROJECT_RUMOR_DESC</Text>
    </RumorFact>

    <ExploreFact>
      <ID>MOD_MUSEUM_FACT</ID>
      <Text>MOD_MUSEUM_FACT_DESC</Text>
    </ExploreFact>
  </Entry>



</AstroObjectEntry>